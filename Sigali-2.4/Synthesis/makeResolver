#!/usr/bin/tcsh
# ----------------------------------------------- 
# This file was automatically generated by cmake. 
# Manual editing not recommended. 
# ----------------------------------------------- 

 if ( ! $?Sigali_root ) then 
   echo "##  ERROR  ## Sigali_root not defined"
   echo "  run <source sigaliEnv> in Sigali root directory"
   exit
 endif

 if (! -edr "${Sigali_Synthesis}") then 
    echo " ERROR : the directory ${Sigali_Synthesis} does not exist"
    exit
 endif
 if ($# > 1) then
    echo "Wrong number of parameter, usage: makeResolver [|clean|all|doc|doc_forced]"
    exit
 endif
 if ($# == 0) then
    set arguments="all"
 else
 if ( ("$1"  != "clean") && ("$1"  != "all") && ("$1"  != "doc") && ("$1"  != "doc_forced") )    then
       echo "improper parameter: $1, usage: makeResolver [|clean|all|doc|doc_forced]"
       exit
    else 
      set arguments=$1
    endif
 endif

 set xx=$PWD 
 cd ${Sigali_Synthesis}/cmake 
 if (! -efr "CMakeCache.txt") then 
    cmake ..
 endif
 make ${arguments} -s -i
 cd $xx 
 unset xx 
