#!/usr/bin/tcsh
# ----------------------------------------------- 
# This file was automatically generated by cmake. 
# Manual editing not recommended. 
# ----------------------------------------------- 

 setenv Sigali_root /home/osboxes/tmc/Sigali-2.4
 setenv Sigali_VERSION 2.3
 if ( -ef "${Sigali_root}/_config_") then
    source ${Sigali_root}/_config_
 endif
if ( ! $?SIGALI_PATH ) setenv SIGALI_PATH "" 
setenv SIGALI_PATH ".:${Sigali_root}/lib:${SIGALI_PATH}" 
 alias sigali '${Sigali_bin}/sigali' 
  echo "-----------------------------------------------------------------------------"
echo " >>>> sigali command is available."
echo " >>>> SIGALI Documentation is available in : ${Sigali_doc} directory "
echo " >>>> Predefined SIGALI Libraries are available in : ${Sigali_lib} directory "
echo "------------------------------------------------------------------------------"
